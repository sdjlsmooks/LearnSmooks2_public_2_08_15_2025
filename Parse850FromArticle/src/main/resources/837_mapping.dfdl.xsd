<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/"
            xmlns:ibmEdiFmt="http://www.ibm.com/dfdl/EDI/Format"
            xmlns:ibmSchExtn="http://www.ibm.com/schema/extensions">

    <xsd:import namespace="http://www.ibm.com/dfdl/EDI/Format"
                schemaLocation="/EDIFACT-Common/IBM_EDI_Format.dfdl.xsd"/>

    <xsd:annotation>
        <xsd:appinfo source="http://www.ogf.org/dfdl/">
            <dfdl:format ref="ibmEdiFmt:EDIFormat"/>
        </xsd:appinfo>
    </xsd:annotation>

    <xsd:element ibmSchExtn:docRoot="true" name="X12_835_Interchange">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element dfdl:initiator="ISA" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="interchange-header"
                             type="ISA_InterchangeHeader"/>
                <xsd:element dfdl:initiator="GS" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="group-header"
                             type="GS_FunctionalGroupHeader"/>
                <xsd:element dfdl:initiator="ST" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="transaction-set-header"
                             type="ST_TransactionSetHeader"/>
                <xsd:element dfdl:initiator="BHT" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="beginning-hierarchical-transaction"
                             type="BHT_BeginningHierarchicalTransaction" minOccurs="0"/>
                
                <!-- Loop 1000A - Submitter Name -->
                <xsd:element name="Loop_1000A_SubmitterName" minOccurs="0" maxOccurs="1">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="submitter-name"
                                         type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="PER" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="submitter-edi-contact-information"
                                         type="PER_Segment" minOccurs="0" maxOccurs="2"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                
                <!-- Loop 1000B - Receiver Name -->
                <xsd:element name="Loop_1000B_ReceiverName" minOccurs="0" maxOccurs="1">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="receiver-name"
                                         type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
                
                <!-- Loop 2000A - Billing Provider Detail -->
                <xsd:element name="Loop_2000A_BillingProviderDetail" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element dfdl:initiator="HL" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-hierarchical-level"
                                         type="HL_Segment" minOccurs="1" maxOccurs="1">
                                <xsd:annotation>
                                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                                        <dfdl:discriminator test="{./hierarchical-level-code eq '20'}" />
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element dfdl:initiator="PRV" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-specialty-information"
                                         type="PRV_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="CUR" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="foreign-currency-information"
                                         type="CUR_Segment" minOccurs="0" maxOccurs="1"/>
                            
                            <!-- Loop 2010AA - Billing Provider Detail -->
                            <xsd:element name="Loop_2010AA_BillingProviderDetailHL" minOccurs="0" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-name"
                                                     type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N3" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-address"
                                                     type="N3_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N4" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-city-state-zip"
                                                     type="N4_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-tax-id"
                                                     type="REF_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-upin-license-info"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="2"/>
                                        <xsd:element dfdl:initiator="PER" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-contact-info"
                                                     type="PER_Segment" minOccurs="0" maxOccurs="2"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            
                            <!-- Loop 2010AB - Pay-To Address Name -->
                            <xsd:element name="Loop_2010AB_PayToAddressName" minOccurs="0" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-address-name"
                                                     type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N3" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-address"
                                                     type="N3_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N4" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-city-state-zip"
                                                     type="N4_Segment" minOccurs="1" maxOccurs="1"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            
                            <!-- Loop 2010AC - Pay-To Plan Name -->
                            <xsd:element name="Loop_2010AC_PayToPlanName" minOccurs="0" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-plan-name"
                                                     type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N3" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-plan-address"
                                                     type="N3_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N4" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-plan-city-state-zip"
                                                     type="N4_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-plan-secondary-id"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="pay-to-plan-tax-id"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="1"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>

                <!-- Loop 2000B - Subscriber Detail -->
                <xsd:element name="Loop_2000B_SubscriberDetail" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element dfdl:initiator="HL" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-hierarchical-level"
                                         type="HL_Segment" minOccurs="1" maxOccurs="1">
                                <xsd:annotation>
                                    <xsd:appinfo source="http://www.ogf.org/dfdl/">
                                        <dfdl:discriminator test="{hierarchical-level-code eq '22'}"/>
                                    </xsd:appinfo>
                                </xsd:annotation>
                            </xsd:element>
                            <xsd:element dfdl:initiator="SBR" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-information"
                                         type="SBR_Segment" minOccurs="1" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="PAT" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="patient-information"
                                         type="PAT_Segment" minOccurs="0" maxOccurs="1"/>
                            
                            <!-- Loop 2010BA - Subscriber Name -->
                            <xsd:element name="Loop_2010BA_SubscriberName" minOccurs="0" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-name"
                                                     type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N3" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-address"
                                                     type="N3_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N4" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-city-state-zip"
                                                     type="N4_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="DMG" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-demographic-info"
                                                     type="DMG_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="subscriber-secondary-id"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="property-and-casualty-claim-number"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="PER" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="property-casualty-contact-info"
                                                     type="PER_Segment" minOccurs="0" maxOccurs="1"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            
                            <!-- Loop 2010BB - Payer Name -->
                            <xsd:element name="Loop_2010BB_PayerName" minOccurs="0" maxOccurs="1">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="payer-name"
                                                     type="NM1_Segment" minOccurs="1" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N3" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="payer-address"
                                                     type="N3_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="N4" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="payer-city-state-zip"
                                                     type="N4_Segment" minOccurs="0" maxOccurs="1"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="payer-secondary-id"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="3"/>
                                        <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="billing-provider-secondary-id"
                                                     type="REF_Segment" minOccurs="0" maxOccurs="2"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>

                <!-- Loop 2300 - Claim Information -->
                <xsd:element name="Loop_2300_ClaimInformation" minOccurs="0" maxOccurs="unbounded">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element dfdl:initiator="CLM" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-information"
                                         type="CLM_Segment" minOccurs="1" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="onset-illness-or-symptom"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="initial-treatment-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="last-seen-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="acute-manifestation-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="accident-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="last-menstrual-period-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="last-x-ray-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="hearing-vision-prescription-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="disability-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="last-worked-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="authorized-return-to-work-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="admission-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="discharge-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="1"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="assumed-relinquished-care-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="2"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="property-casualty-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="2"/>
                            <xsd:element dfdl:initiator="DTP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="repricer-received-date"
                                         type="DTP_Segment" minOccurs="0" maxOccurs="2"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>

                <xsd:element dfdl:initiator="SE" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="transaction-set-trailer"
                             type="SE_TransactionSetTrailer"/>
                <xsd:element dfdl:initiator="GE" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="functional-group-trailer"
                             type="GE_FunctionalGroupTrailer"/>
                <xsd:element dfdl:initiator="IEA" dfdl:ref="ibmEdiFmt:EDISegmentFormat"
                             name="interchange-control-trailer" type="IEA_InterchangeTrailer"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <!-- Segment Type Definitions -->
    <xsd:complexType name="ISA_InterchangeHeader">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="auth-qual" type="alpha2-2"/>
            <xsd:element name="auth-id" type="alpha10-10"/>
            <xsd:element name="security-qual" type="alpha2-2"/>
            <xsd:element name="security-id" type="alpha10-10"/>
            <xsd:element name="sender-qual" type="alpha2-2"/>
            <xsd:element name="sender-id" type="alpha15-15"/>
            <xsd:element name="receiver-qual" type="alpha2-2"/>
            <xsd:element name="receiver-id" type="alpha15-15"/>
            <xsd:element name="date" type="alpha6-6"/>
            <xsd:element name="time" type="numeric4-4"/>
            <xsd:element name="standard" type="alpha1-1"/>
            <xsd:element name="version" type="numeric5-5"/>
            <xsd:element name="interchange-control-number" type="numeric9-9"/>
            <xsd:element name="ack" type="numeric1-1"/>
            <xsd:element name="test" type="alpha1-1"/>
            <xsd:element name="s-delimiter" type="alpha1-1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="GS_FunctionalGroupHeader">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="code" type="alpha2-2"/>
            <xsd:element name="sender" type="alpha2-15"/>
            <xsd:element name="receiver" type="alpha2-15"/>
            <xsd:element name="date" type="alpha8-8"/>
            <xsd:element name="time" type="numeric4-8"/>
            <xsd:element name="group-control-number" type="numeric1-9"/>
            <xsd:element name="standard" type="alpha1-2"/>
            <xsd:element name="version" type="alpha1-12"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="ST_TransactionSetHeader">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="code" type="numeric3-3"/>
            <xsd:element name="transaction-set-control-number" type="alpha4-9"/>
            <xsd:element name="implementation-convention-reference" type="alpha1-35" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BHT_BeginningHierarchicalTransaction">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="hierarchical-structure-code" type="alpha4-4"/>
            <xsd:element name="transaction-set-purpose-code" type="alpha2-2"/>
            <xsd:element name="reference-identification" type="alpha1-50" minOccurs="0"/>
            <xsd:element name="date" type="alpha8-8" minOccurs="0"/>
            <xsd:element name="time" type="alpha4-8" minOccurs="0"/>
            <xsd:element name="transaction-type-code" type="alpha2-2" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="BPR_Segment">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="transaction-handling-code" type="alpha1-2"/>
            <xsd:element name="monetary-amount" type="decimal1-18"/>
            <xsd:element name="credit-debit-flag-code" type="alpha1-1"/>
            <xsd:element name="payment-method-code" type="alpha3-3"/>
            <xsd:element name="payment-format-code" type="alpha1-3" minOccurs="0"/>
            <xsd:element name="depository-financial-institution-dfi-identification-number-qualifier" type="alpha2-2" minOccurs="0"/>
            <xsd:element name="depository-financial-institution-dfi-identification-number" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="account-number-qualifier" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="sender-bank-account-number" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="originating-company-identifier" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="originating-company-supplemental-code" type="alpha1-10" minOccurs="0"/>
            <xsd:element name="receiver-or-provider-bank-id-number-qualifier" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="receiver-or-provider-bank-id-number" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="receiver-or-provider-account-number-qualifier" type="alpha1-2" minOccurs="0"/>
            <xsd:element name="receiver-or-provider-account-number" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="date" type="alpha8-8" minOccurs="0"/>
            <xsd:element name="business-function-code" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="dfi-id-number-qualifier" type="alpha1-2" minOccurs="0"/>
            <xsd:element name="dfi-identification-number" type="alpha1-17" minOccurs="0"/>
            <xsd:element name="account-number-qualifier-2" type="alpha1-3" minOccurs="0"/>
            <xsd:element name="sender-bank-account-number-2" type="alpha1-35" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="TRN_Segment">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="trace-type-code" type="alpha1-2"/>
            <xsd:element name="reference-identification" type="alpha1-50"/>
            <xsd:element name="originating-company-identifier" type="alpha10-10" minOccurs="0"/>
            <xsd:element name="reference-identification-2" type="alpha1-50" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CUR_Segment">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="entity-identifier-code" type="alpha2-3"/>
            <xsd:element name="currency-code" type="alpha3-3"/>
            <xsd:element name="exchange-rate" type="decimal4-10" minOccurs="0"/>
            <xsd:element name="entity-identifier-code-2" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="currency-code-2" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="currency-market-exchange-code" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="date-time-qualifier" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="date" type="alpha8-8" minOccurs="0"/>
            <xsd:element name="time" type="alpha4-8" minOccurs="0"/>
            <xsd:element name="date-time-qualifier-2" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="date-2" type="alpha8-8" minOccurs="0"/>
            <xsd:element name="time-2" type="alpha4-8" minOccurs="0"/>
            <xsd:element name="date-time-qualifier-3" type="alpha3-3" minOccurs="0"/>
            <xsd:element name="date-time-period" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="date-time-period-format-qualifier" type="alpha2-3" minOccurs="0"/>
            <xsd:element name="date-time-period-2" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="date-time-period-format-qualifier-2" type="alpha2-3" minOccurs="0"/>
            <xsd:element name="date-time-period-3" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="date-time-period-format-qualifier-3" type="alpha2-3" minOccurs="0"/>
            <xsd:element name="date-time-period-4" type="alpha1-35" minOccurs="0"/>
            <xsd:element name="date-time-period-format-qualifier-4" type="alpha2-3" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="HL_Segment">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="hierarchical-id-number" type="alpha1-12"/>
            <xsd:element name="hierarchical-parent-id-number" type="alpha1-12" minOccurs="0"/>
            <xsd:element name="hierarchical-level-code" type="alpha1-2"/>
            <xsd:element name="hierarchical-child-code" type="alpha1-1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="PRV_Segment">
        <xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat">
            <xsd:element name="provider-code" type="alpha1-3"/>
            <xsd:element name="reference-identification-qualifier" type="alpha2-3"/>
