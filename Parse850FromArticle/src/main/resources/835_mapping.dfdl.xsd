    <xsd:complexType name="DTM_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="DTM01" type="alpha3-3"/><xsd:element name="DTM02" type="alpha8-8"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="Name_Segment_Type"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="N101" type="alpha2-3"/><xsd:element name="N102" type="alpha1-60" minOccurs="0"/><xsd:element name="N103" type="alpha1-80" minOccurs="0"/><xsd:element name="N104" type="alpha2-80" minOccurs="0"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="Address_Segment_Type"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="N301" type="alpha1-55"/><xsd:element name="N302" type="alpha1-55" minOccurs="0"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="Location_Segment_Type"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="N401" type="alpha2-30"/><xsd:element name="N402" type="alpha2-2" minOccurs="0"/><xsd:element name="N403" type="numeric3-15" minOccurs="0"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="PER_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="PER01" type="alpha2-2"/><xsd:element name="PER02" type="alpha1-60" minOccurs="0"/><xsd:element name="PER03" type="alpha2-2"/><xsd:element name="PER04" type="alpha1-256"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="LX_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="LX01" type="numeric1-6"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="TS3_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="TS301" type="alpha2-80"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="TS2_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="TS201" type="decimal1-18"/></xsd:sequence></xsd:complexType>
