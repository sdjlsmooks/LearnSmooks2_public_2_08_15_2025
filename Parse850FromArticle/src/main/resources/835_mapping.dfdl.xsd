                </xsd:element>

                <xsd:element dfdl:initiator="SE" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="transaction-set-trailer" type="SE_TransactionSetTrailer"/>
                <xsd:element dfdl:initiator="GE" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="functional-group-trailer" type="GE_FunctionalGroupTrailer"/>
                <xsd:element dfdl:initiator="IEA" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="interchange-control-trailer" type="IEA_InterchangeTrailer"/>
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

