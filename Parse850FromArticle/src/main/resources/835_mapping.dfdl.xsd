    <xsd:complexType name="QTY_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="QTY01" type="alpha2-2"/><xsd:element name="QTY02" type="decimal1-15"/></xsd:sequence></xsd:complexType>
