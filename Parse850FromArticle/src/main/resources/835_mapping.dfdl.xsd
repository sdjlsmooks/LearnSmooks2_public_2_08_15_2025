    <xsd:simpleType name="decimal1-15"><xsd:restriction base="xsd:string"><xsd:pattern value="-?[0-9]{1,13}(\.[0-9]{1,2})?"/></xsd:restriction></xsd:simpleType>
    <xsd:simpleType name="decimal1-18"><xsd:restriction base="xsd:string"><xsd:pattern value="-?[0-9]{1,16}(\.[0-9]{1,2})?"/></xsd:restriction></xsd:simpleType>
