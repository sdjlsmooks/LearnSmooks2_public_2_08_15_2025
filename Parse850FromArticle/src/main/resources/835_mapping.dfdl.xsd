    <xsd:complexType name="CAS_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="CAS01" type="alpha1-2"/><xsd:element name="CAS02" type="alpha1-5"/><xsd:element name="CAS03" type="decimal1-18"/></xsd:sequence></xsd:complexType>
    <xsd:complexType name="MIA_Segment"><xsd:sequence dfdl:ref="ibmEdiFmt:EDISegmentSequenceFormat"><xsd:element name="MIA01" type="decimal1-18"/></xsd:sequence></xsd:complexType>
