                                            <xsd:complexType>
                                                <xsd:sequence>
                                                    <xsd:element dfdl:initiator="CLP" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-payment-information" type="CLP_Segment"/>
                                                    <xsd:element dfdl:initiator="CAS" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-adjustment" type="CAS_Segment" minOccurs="0" maxOccurs="99"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="patient-name" type="Name_Segment_Type"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="insured-name" type="Name_Segment_Type" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="corrected-patient-insured" type="Name_Segment_Type" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-provider-name" type="Name_Segment_Type" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="crossover-carrier-name" type="Name_Segment_Type" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="NM1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="corrected-priority-payer-name" type="Name_Segment_Type" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="MIA" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="inpatient-adjudication" type="MIA_Segment" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="MOA" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="outpatient-adjudication" type="MOA_Segment" minOccurs="0"/>
                                                    <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="other-claim-related-id" type="REF_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                    <xsd:element dfdl:initiator="DTM" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-level-dates" type="DTM_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                    <xsd:element dfdl:initiator="PER" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-contact-info" type="PER_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                    <xsd:element dfdl:initiator="AMT" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="claim-level-amounts" type="AMT_Segment" minOccurs="0" maxOccurs="unbounded"/>
