    <xsd:simpleType name="numeric6-6"><xsd:restriction base="xsd:string"><xsd:pattern value="[0-9]{6}"/></xsd:restriction></xsd:simpleType>
    <xsd:simpleType name="numeric8-8"><xsd:restriction base="xsd:string"><xsd:pattern value="[0-9]{8}"/></xsd:restriction></xsd:simpleType>
