                            <xsd:element name="Loop_1000A_Payer">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element dfdl:initiator="N1" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="payer-identification" type="Name_Segment_Type"/>
