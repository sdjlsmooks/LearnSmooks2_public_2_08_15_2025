                                                    <xsd:element name="Loop_2110_ServicePayment" minOccurs="0" maxOccurs="999">
                                                        <xsd:complexType>
                                                            <xsd:sequence>
                                                                <xsd:element dfdl:initiator="SVC" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-payment-information" type="SVC_Segment"/>
                                                                <xsd:element dfdl:initiator="DTM" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-date" type="DTM_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                                <xsd:element dfdl:initiator="CAS" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-adjustment" type="CAS_Segment" minOccurs="0" maxOccurs="99"/>
                                                                <xsd:element dfdl:initiator="REF" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-identification" type="REF_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                                <xsd:element dfdl:initiator="AMT" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-level-amount" type="AMT_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                                <xsd:element dfdl:initiator="QTY" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="service-level-quantity" type="QTY_Segment" minOccurs="0" maxOccurs="unbounded"/>
                                                                <xsd:element dfdl:initiator="LQ" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="healthcare-remark-codes" type="LQ_Segment" minOccurs="0" maxOccurs="99"/>
                                                            </xsd:sequence>
                                                        </xsd:complexType>
                                                    </xsd:element>
                                                </xsd:sequence>
                                            </xsd:complexType>
                                        </xsd:element>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                            <xsd:element dfdl:initiator="PLB" dfdl:ref="ibmEdiFmt:EDISegmentFormat" name="provider-adjustment" type="PLB_Segment" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
