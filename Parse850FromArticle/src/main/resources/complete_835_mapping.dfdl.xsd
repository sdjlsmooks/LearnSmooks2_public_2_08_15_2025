    <xsd:simpleType name="numeric5-5">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{5}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="numeric6-6">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{6}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="numeric8-8">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{8}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="numeric9-9">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{9}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <!-- Decimal Types -->
    <xsd:simpleType name="decimal1-15">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="-?[0-9]{1,13}(\.[0-9]{1,2})?"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="decimal1-18">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="-?[0-9]{1,16}(\.[0-9]{1,2})?"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="decimal4-10">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="-?[0-9]{1,8}(\.[0-9]{1,2})?"/>
        </xsd:restriction>
    </xsd:simpleType>

</xsd:schema>